{
    "name": "NestJS Server",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "server",
    "workspaceFolder": "/workspace",
    "shutdownAction": "none",
    "forwardPorts": [
        3000
    ],
    "containerEnv": {
        "PORT": "3000",
        "POSTGRES_HOST": "database",
        "POSTGRES_PORT": "5432",
        "POSTGRES_USER": "postgres",
        "POSTGRES_PASSWORD": "postgres",
        "POSTGRES_DB": "postgres",
        "DB_SYNC": "true"
    }
}